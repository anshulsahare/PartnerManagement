<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>prevent_blank_last_name_agent</fullName>
    <active>false</active>
    <errorConditionFormula>AND(ISBLANK( Last_Name__c ),$Profile.Id = &apos;Agent&apos;)</errorConditionFormula>
    <errorDisplayField>Last_Name__c</errorDisplayField>
    <errorMessage>prevent applicant if last name is blank</errorMessage>
</ValidationRule>
