<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>prevent_DOB_month_august</fullName>
    <active>false</active>
    <description>prevent applicant if DOB month is august</description>
    <errorConditionFormula>MONTH( DOB__c ) ==8</errorConditionFormula>
    <errorDisplayField>DOB__c</errorDisplayField>
    <errorMessage>DOB month should not be august</errorMessage>
</ValidationRule>
