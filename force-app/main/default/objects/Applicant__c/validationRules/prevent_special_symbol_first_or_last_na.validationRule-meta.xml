<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>prevent_special_symbol_first_or_last_na</fullName>
    <active>false</active>
    <errorConditionFormula>or(NOT(REGEX(  First_Name__c , &quot;^[a-zA-Z0-9_]*$&quot;)),
NOT(REGEX(  Last_Name__c , &quot;^[a-zA-Z0-9_]*$&quot;)))</errorConditionFormula>
    <errorMessage>error in symbols of first or last name</errorMessage>
</ValidationRule>
