<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>prevent_blank_last_name</fullName>
    <active>false</active>
    <description>prevent applicant , if last name is blank</description>
    <errorConditionFormula>ISBLANK( Last_Name__c )</errorConditionFormula>
    <errorDisplayField>Last_Name__c</errorDisplayField>
    <errorMessage>last name cannot be blank</errorMessage>
</ValidationRule>
